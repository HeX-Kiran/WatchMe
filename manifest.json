{
    "name": "Watch Me",
    "version": "0.0.0.1",
    "manifest_version": 3,
    "description": "A basic extension to track your web surfs with a personlised UI dashboard",
    "icons": {
        "16": "icons/favicon-16x16.png",
        "32": "icons/favicon-32x32.png",
        "192": "icons/android-chrome-192x192.png"
    },
    "action": {
        "default_popup": "popup/popup.html"
    },
    "permissions": [
        "tabs",
        "storage",
        "idle"
    ],
    "background": {
        "service_worker": "background.js"
    }
}